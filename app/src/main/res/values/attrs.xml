<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AwesomeTextInputLayout">
        <attr name="borderColor" format="color" />
        <attr name="hintColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="cornerRadius" format="dimension" />
        <attr name="animatedTextSize" format="dimension" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>
    <declare-styleable name="ShimmerLayout">
        <attr name="shimmer_color" format="color|reference" />
        <attr name="shimmer_animation_duration" format="integer|reference" />
        <attr name="shimmer_auto_start" format="boolean|reference" />
        <attr name="shimmer_angle" format="integer|reference" />
        <attr name="shimmer_mask_width" format="float|reference" />
        <attr name="shimmer_gradient_center_color_width" format="float|reference" />
        <attr name="shimmer_reverse_animation" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="CustomCalendar">
        <attr name="month_year_format" format="enum">
            <enum name="three_letter_month__with_year" value="0" />
            <enum name="full_month__with_year" value="1" />
        </attr>
        <attr name="day_of_week_length" format="integer" />
        <attr name="row_height" format="dimension" />
        <attr name="day_of_week_start_from" format="enum">
            <enum name="sunday" value="0" />
            <enum name="monday" value="1" />
            <enum name="tuesday" value="2" />
            <enum name="wednesday" value="3" />
            <enum name="thursday" value="4" />
            <enum name="friday" value="5" />
            <enum name="saturday" value="6" />
        </attr>
        <attr name="left_button_src" format="reference" />
        <attr name="right_button_src" format="reference" />
    </declare-styleable>

    <declare-styleable name="ContinuousSliderDff">
        <attr name="android:stepSize" format="float" />
        <attr name="android:valueFrom" format="float" />
        <attr name="android:valueTo" format="float" />
        <attr name="android:value" format="float" />
        <attr name="trackColorActive" format="color" />
        <attr name="trackColorInActive" format="color" />
        <attr name="tickColorActive" format="color" />
        <attr name="tickColorInactive" format="color" />
        <attr name="sliderLabel" format="boolean" />
        <attr name="sliderLabelText" format="string" />
        <attr name="trackHeight" />
        <attr name="thumbColor" />
        <attr name="thumbRadius" />
        <attr name="thumbStrokeWidth" />
        <attr name="thumbStrokeColor" />
        <attr name="thumbTextPadding" format="dimension" />
        <attr name="android:thumbOffset" />
        <attr name="decimalFormat" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CustomTextField">
        <attr name="ti_labelText" format="string" />
        <attr name="ti_cornerRadius" format="dimension" />
        <attr name="ti_borderColor" format="color" />
        <attr name="ti_cardElevation" format="dimension" />
        <attr name="ti_startDrawable" format="reference" />
        <attr name="ti_endDrawable" format="reference" />
    </declare-styleable>


    <declare-styleable name="CardTextInputLayout">
        <!-- TextInputLayout attributes -->
        <attr name="ctl_hint" format="string" />
        <attr name="ctl_hintEnabled" format="boolean" />
        <attr name="ctl_hintTextAppearance" format="reference" />
        <attr name="ctl_hintTextColor" format="color" />
        <attr name="ctl_boxBackgroundMode">
            <enum name="none" value="0" />
            <enum name="filled" value="1" />
            <enum name="outline" value="2" />
        </attr>
        <attr name="ctl_boxBackgroundColor" format="color" />
        <attr name="ctl_boxCornerRadiusTopStart" format="dimension" />
        <attr name="ctl_boxCornerRadiusTopEnd" format="dimension" />
        <attr name="ctl_boxCornerRadiusBottomStart" format="dimension" />
        <attr name="ctl_boxCornerRadiusBottomEnd" format="dimension" />
        <attr name="ctl_boxStrokeColor" format="color" />
        <attr name="ctl_boxStrokeWidth" format="dimension" />
        <attr name="ctl_boxStrokeWidthFocused" format="dimension" />
        <attr name="ctl_counterEnabled" format="boolean" />
        <attr name="ctl_counterMaxLength" format="integer" />
        <attr name="ctl_counterTextAppearance" format="reference" />
        <attr name="ctl_counterOverflowTextAppearance" format="reference" />
        <attr name="ctl_errorEnabled" format="boolean" />
        <attr name="ctl_errorTextAppearance" format="reference" />
        <attr name="ctl_helperTextEnabled" format="boolean" />
        <attr name="ctl_helperText" format="string" />
        <attr name="ctl_helperTextTextAppearance" format="reference" />
        <attr name="ctl_placeholderText" format="string" />
        <attr name="ctl_placeholderTextAppearance" format="reference" />
        <attr name="ctl_prefixText" format="string" />
        <attr name="ctl_prefixTextAppearance" format="reference" />
        <attr name="ctl_suffixText" format="string" />
        <attr name="ctl_suffixTextAppearance" format="reference" />
        <attr name="ctl_startIconDrawable" format="reference" />
        <attr name="ctl_startIconContentDescription" format="string" />
        <attr name="ctl_endIconMode">
            <enum name="custom" value="-1" />
            <enum name="none" value="0" />
            <enum name="password_toggle" value="1" />
            <enum name="clear_text" value="2" />
            <enum name="dropdown_menu" value="3" />
        </attr>
        <attr name="ctl_endIconDrawable" format="reference" />
        <attr name="ctl_endIconContentDescription" format="string" />

        <!-- TextInputEditText attributes -->
        <attr name="ctl_inputType" format="flags">
            <flag name="none" value="0x00000000" />
            <flag name="text" value="0x00000001" />
            <flag name="textCapCharacters" value="0x00001001" />
            <flag name="textCapWords" value="0x00002001" />
            <flag name="textCapSentences" value="0x00004001" />
            <flag name="textAutoCorrect" value="0x00008001" />
            <flag name="textAutoComplete" value="0x00010001" />
            <flag name="textMultiLine" value="0x00020001" />
            <flag name="textImeMultiLine" value="0x00040001" />
            <flag name="textNoSuggestions" value="0x00080001" />
            <flag name="textUri" value="0x00000011" />
            <flag name="textEmailAddress" value="0x00000021" />
            <flag name="textEmailSubject" value="0x00000031" />
            <flag name="textShortMessage" value="0x00000041" />
            <flag name="textLongMessage" value="0x00000051" />
            <flag name="textPersonName" value="0x00000061" />
            <flag name="textPostalAddress" value="0x00000071" />
            <flag name="textPassword" value="0x00000081" />
            <flag name="textVisiblePassword" value="0x00000091" />
            <flag name="textWebEditText" value="0x000000a1" />
            <flag name="textFilter" value="0x000000b1" />
            <flag name="textPhonetic" value="0x000000c1" />
            <flag name="textWebEmailAddress" value="0x000000d1" />
            <flag name="textWebPassword" value="0x000000e1" />
            <flag name="number" value="0x00000002" />
            <flag name="numberSigned" value="0x00001002" />
            <flag name="numberDecimal" value="0x00002002" />
            <flag name="numberPassword" value="0x00000012" />
            <flag name="phone" value="0x00000003" />
            <flag name="datetime" value="0x00000004" />
            <flag name="date" value="0x00000014" />
            <flag name="time" value="0x00000024" />
        </attr>
        <attr name="ctl_text" format="string" />
        <attr name="ctl_textColor" format="color" />
        <attr name="ctl_textSize" format="dimension" />
        <attr name="ctl_textStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="ctl_maxLines" format="integer" />
        <attr name="ctl_minLines" format="integer" />
        <attr name="ctl_maxLength" format="integer" />
        <attr name="ctl_imeOptions">
            <flag name="normal" value="0x00000000" />
            <flag name="actionUnspecified" value="0x00000000" />
            <flag name="actionNone" value="0x00000001" />
            <flag name="actionGo" value="0x00000002" />
            <flag name="actionSearch" value="0x00000003" />
            <flag name="actionSend" value="0x00000004" />
            <flag name="actionNext" value="0x00000005" />
            <flag name="actionDone" value="0x00000006" />
            <flag name="actionPrevious" value="0x00000007" />
        </attr>
        <attr name="ctl_singleLine" format="boolean" />
    </declare-styleable>

</resources>